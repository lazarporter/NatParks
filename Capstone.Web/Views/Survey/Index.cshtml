@model List<KeyValuePair<string, int>>
@{
    ViewData["Title"] = "Survey";
}

<h2>Index</h2>
<h4>Your feedback is very important to us! Please consider taking our brief survey to help use better understand how to serve you.</h4>
<a class="btn btn-primary" asp-controller="Survey" asp-action="TakeSurvey">Take Survey</a>
<h2><strong>Current Rankings</strong></h2>
<div class="tableFlex">    
    <table class="table table-hover table-bordered table-rounded surveyResultsTable">
        <thead>
            <tr>
                <th>Park</th>
                <th>Votes</th>
            </tr>
        </thead>
        <tbody>
            @foreach (KeyValuePair<string, int> kvp in Model)
            {
                <tr>
                    <td>@kvp.Key</td>
                    <td>@kvp.Value</td>
                </tr>
            }
        </tbody>
    </table>
</div>